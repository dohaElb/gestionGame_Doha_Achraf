<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:controller="com.example.gestiongames.Ajouter" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" prefHeight="400.0" prefWidth="600.0">
    <VBox alignment="CENTER" prefHeight="356.0" prefWidth="332.0" spacing="20.0">
        <Label text="Ajouter Game" textFill="#ef0a0a">
            <font>
                <Font name="System Bold" size="20.0" />
            </font>
        </Label>
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </padding>
        <Label text="Nom" />
        <TextField fx:id="nameField" />

        <Label text="NomDeveloppeur" />
        <TextField fx:id="developerField" />

        <Label text="Platforme" />
        <TextField fx:id="platformField" />

        <Label text="Genre" />
        <TextField fx:id="genreField" />

        <Label text="anneeDeSortie" />
        <TextField fx:id="releaseYearField" />

        <Button text="Ajouter" fx:id="Ajouter" onAction="#initialize2" />

        <Label fx:id="welcomeText" />
    </VBox>
</AnchorPane>

